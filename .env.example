# Cloudflare Configuration
CF_ACCOUNT_ID=your-account-id
CF_API_TOKEN=your-api-token

# R2 Storage
R2_BUCKET_NAME=cloudnotes-dev-storage

# D1 Database
D1_DATABASE_ID=your-database-id
D1_DATABASE_NAME=cloudnotes-db-dev

# AutoRAG
AUTORAG_INSTANCE_ID=your-autorag-id

# Cloudflare Queues
QUEUE_NAME=note-processing-dev

# JWT Secret (generate a secure random string)
JWT_SECRET=your-jwt-secret-key

# Environment
NODE_ENV=development
APP_URL=http://localhost:3000

# Optional: Cloudflare Access (for OAuth)
CF_ACCESS_CLIENT_ID=your-access-client-id
CF_ACCESS_CLIENT_SECRET=your-access-client-secret

# Optional: External AI API Keys (if using external services)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key